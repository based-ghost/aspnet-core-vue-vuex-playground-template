{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "src/**/**/*.tsx",
      "src/**/**/*.ts",
      "src/**/*.tsx",
      "src/*.tsx"
    ]
  },
  "rules": {
    "quotemark": false,
    "ordered-imports": false,
    "member-ordering": false,
    "trailing-comma": false,
    "eofline": false,
    "max-line-length": false,
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  }
}